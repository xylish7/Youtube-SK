{
  "name": "youtube-sk",
  "version": "1.0.0",
  "description": "A simple interface app used to download, convert or cut music from youtube but you can use it as a music player too.",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder -w",
    "dist": "npm run build && electron-builder",
    "publish": "npm run build && electron-builder --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "YouTube-SK",
    "appId": "org.Youtube-SK",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "nsis": {
      "installerIcon": "assets/youtube-sk.ico",
      "uninstallerIcon": "assets/youtube-sk.ico",
      "uninstallDisplayName": "Youtube-SK"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/youtube-sk.ico"
    },
    "publish": {
      "provider": "github",
      "owner": "xylish7",
      "repo": "Youtube-SK",
      "private": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Robinfr/electron-react-typescript.git"
  },
  "author": {
    "name": "Filip Frincu",
    "email": "filip.frincu@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "electron",
    "youtube",
    "download",
    "convert",
    "cut",
    "music",
    "player",
    "video"
  ],
  "bugs": {
    "url": "https://github.com/Robinfr/electron-react-typescript/issues"
  },
  "homepage": "https://github.com/Robinfr/electron-react-typescript",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webdriverio": "^5.0.0",
    "@types/webpack-env": "^1.15.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.9",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^9.0.2",
    "source-map-loader": "^1.0.1",
    "spectron": "^11.1.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.3",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "antd": "^4.4.3",
    "antd-theme-webpack-plugin": "^1.3.6",
    "custom-electron-titlebar": "^3.2.3",
    "electron-log": "^4.2.2",
    "electron-updater": "^4.3.1",
    "hazardous": "^0.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^2.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "youtube-dl": "^3.0.2"
  }
}
